// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Page {
  id        String   @id @default(uuid())
  imageUrl  String[]
  pin       String
  path      String   @unique
  body      String
  createdAt DateTime @default(now())
}
